PAYMENTS_BASE_URL=http://host.docker.internal:8080
PAYMENTS_INTERNAL_SECRET=<REPLACE_WITH_INTERNAL_API_SECRET_FROM_PAYMENTS>
PLATFORM_FEE_BPS=0
FEE_WALLET_PHONE=+963999999999
PAYMENTS_WEBHOOK_SECRET=<REPLACE_WITH_SECRET>
